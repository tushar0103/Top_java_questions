 public static void main(String[] args) {
        String s = "tushar kumar chauhan";
        String st = "", ans = "";
        for (int i = s.length() - 1; i >= 0; i--) {
            if (s.charAt(i) != ' ') {
                st = s.charAt(i) + st;
            } else {
                ans = ans + " " + st;
                st = "";
            }
        }
        ans = ans + " " + st;
        System.out.println(ans);

    }
